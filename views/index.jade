html
  head(lang="en")
    title Stock Ticker!!!
    script(type="text/javascript", src="/javascripts/jquery-1.7.1.js")
    script(type="text/javascript", src="/javascripts/jquery.webticker.js")
    script(type="text/javascript")
      $(function() {
