    link(rel="stylesheet",type="text/css",href="/stylesheets/webticker.css")
    script(type='text/javascript', src="/socket.io/socket.io.js")
    script(type='text/javascript')
      var socket = io.connect();

      socket.on('stock_update', function(data) {
        var ticker = $("#webTicker");
        //ticker.webTicker('stop');
        $("#webTicker > li").remove();
        for(symbol in data) {
          ticker.prepend("<li>" + symbol + ": " + data[symbol] + "</li>");
        }
        //ticker.webTicker('cont');
      });

      setInterval(function() {
        socket.emit('request_stock_update');
      }, 60000);
      socket.emit('request_stock_update');
    body
